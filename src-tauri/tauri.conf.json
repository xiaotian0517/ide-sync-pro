  "productName": "IDE Sync Pro",
  "version": "1.0.0",
  "identifier": "com.tianhaonan.ide-sync-pro",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "IDE Sync Pro",
        "width": 900,
        "height": 600,
